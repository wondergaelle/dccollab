<div class="container">
    {{ form_start(form) }}

    <div class="form-group">

        {{ form_row(form.nom) }} <br>

        {{ form_row(form.user) }} <br>

        {{ form_row(form.dateCreation) }} <br>

        {{ form_row(form.nomEntreprise) }} <br>

        {{ form_row(form.contenu) }} <br>

        {{ form_row(form.extrait) }} <br>

        {{ form_row(form.image) }} <br>

        {{ form_row(form.competence) }} <br>

    </div>
    <div>
        <button type="submit" class="btn btn-danger">{{ button_label|default('Sauvegarder') }}</button>
    </div>
</div>
{{ form_end(form) }}


